{
  "name": "@rqt/namecheap",
  "version": "2.1.1",
  "description": "An Implementation Of The Namecheap.com API.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask -t 30000",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "yarn-s doc1 doc2 doc3 doc4",
    "doc1": "NODE_DEBUG=doc doc documentary/index -o README.md -n _namecheap",
    "doc2": "NODE_DEBUG=doc doc documentary/1-domains -o doc/DOMAINS.md -n _namecheap",
    "doc3": "NODE_DEBUG=doc doc documentary/2-users -o doc/USERS.md -n _namecheap",
    "doc4": "NODE_DEBUG=doc doc documentary/3-address -o doc/ADDRESS.md -n _namecheap",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 d2 externs",
    "d1": "typal src -c",
    "d2": "typal types/typedefs -c",
    "build": "yarn-s d b doc",
    "p": "node price",
    "e": "alanode",
    "example/": "yarn e example/example.js",
    "externs": "typal externs.js -e"
  },
  "files": [
    "build",
    "externs.js"
  ],
  "externs": "externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/namecheap.git"
  },
  "keywords": [
    "namecheap",
    "rqt",
    "request",
    "godaddy",
    "gandi",
    "api",
    "domain",
    "domains",
    "register",
    "registration",
    "registrar",
    "whois",
    "certificate",
    "ssl"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/namecheap/issues"
  },
  "homepage": "https://nameexpensive.com",
  "devDependencies": {
    "@wrote/read": "^1.0.4",
    "@wrote/write": "^1.0.4",
    "alamode": "^2.2.0",
    "bosom": "^1.0.0",
    "documentary": "^1.27.3",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "rexml": "^1.4.0",
    "rqt": "^3.1.3"
  }
}
