<types namespace="_namecheap">
  <type name="AddressDetail" noToc>
    <prop string name="EmailAddress">
      Email address of the user.
    </prop>
    <prop string name="FirstName">
      First name of the user.
    </prop>
    <prop string name="LastName">
      Last name of the user.
    </prop>
    <prop opt string name="JobTitle">
      Job designation of the user
    </prop>
    <prop opt string name="Organization">
      Organization of the user.
    </prop>
    <prop string name="Address1">
      StreetAddress1 of the user.
    </prop>
    <prop opt string name="Address2">
      StreetAddress2 of the user.
    </prop>
    <prop string name="City">
      City of the user.
    </prop>
    <prop string name="StateProvince">
      State/Province of the user.
    </prop>
    <prop string name="StateProvinceChoice">
      State/Province choice of the user. Either `'S'` or `'P'`.
    </prop>
    <prop string name="Zip">
      Zip/Postal code of the user.
    </prop>
    <prop string name="Country">
      Two letter country code of the user.
    </prop>
    <prop string name="Phone">
      Phone number in the format `+NNN.NNNNNNNNNN`.
    </prop>
    <prop opt string name="PhoneExt">
      PhoneExt of the user.
    </prop>
    <prop opt string name="Fax">
      Fax number in the format `+NNN.NNNNNNNNNN`.
    </prop>
  </type>
</types>