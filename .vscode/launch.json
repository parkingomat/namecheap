{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "@rqt/namecheap,aqt",
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "env": {
        "NODE_DEBUG": "@rqt/namecheap,aqt",
        "ALAMODE_ENV": "test-compile",
        "DEBUG": "1",
      },
      "sourceMaps": false,
      "console": "integratedTerminal",
      "args": [
        // "example/run/users/get-pricing.js"
        // "example"
        "example/run/dns/set-hosts"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Wiki",
      "program": "${workspaceFolder}/node_modules/.bin/doc-dev",
      "env": {
        "NODE_DEBUG": "@rqt/namecheap,aqt,doc",
        "DEBUG": "1",
      },
      "console": "integratedTerminal",
      "args": [
        // "example/run/users/get-pricing.js"
        // "example/run/domains/create.js",
        "wiki", "-n", "_namecheap", "-W", "../namecheap.wiki",
        "-f", "DNS"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Doc",
      "program": "${workspaceFolder}/node_modules/.bin/doc-dev",
      "env": {
        "NODE_DEBUG": "@rqt/namecheap,aqt,doc",
        "DEBUG": "1",
      },
      "console": "integratedTerminal",
      "args": [
        "-o", "README.md", "-n", "_namecheap"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Price",
      "program": "${workspaceFolder}/price/index.js",
      "env": {
        "NODE_DEBUG": "@rqt/namecheap,aqt",
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
